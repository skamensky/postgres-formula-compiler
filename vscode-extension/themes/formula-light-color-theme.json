{
  "name": "Formula Light",
  "type": "light",
  "colors": {
    "editor.background": "#ffffff",
    "editor.foreground": "#000000",
    "editorLineNumber.foreground": "#237893",
    "editorLineNumber.activeForeground": "#0B216C"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment.line.double-slash.formula",
        "comment.block.formula"
      ],
      "settings": {
        "foreground": "#008000",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string.quoted.double.formula"
      ],
      "settings": {
        "foreground": "#A31515"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric.formula"
      ],
      "settings": {
        "foreground": "#098658"
      }
    },
    {
      "name": "Boolean Literals",
      "scope": [
        "constant.language.boolean.formula"
      ],
      "settings": {
        "foreground": "#0000FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "NULL Literal",
      "scope": [
        "constant.language.null.formula"
      ],
      "settings": {
        "foreground": "#0000FF",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Core Functions",
      "scope": [
        "keyword.function.core.formula"
      ],
      "settings": {
        "foreground": "#795E26",
        "fontStyle": "bold"
      }
    },
    {
      "name": "String Functions",
      "scope": [
        "keyword.function.string.formula"
      ],
      "settings": {
        "foreground": "#267f99",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Null Functions",
      "scope": [
        "keyword.function.null.formula"
      ],
      "settings": {
        "foreground": "#AF00DB",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Logical Functions",
      "scope": [
        "keyword.function.logical.formula"
      ],
      "settings": {
        "foreground": "#FF8000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Math Functions",
      "scope": [
        "keyword.function.math.formula"
      ],
      "settings": {
        "foreground": "#001080",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Date Functions",
      "scope": [
        "keyword.function.date.formula"
      ],
      "settings": {
        "foreground": "#CD3131",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Aggregate Functions",
      "scope": [
        "keyword.function.aggregate.formula"
      ],
      "settings": {
        "foreground": "#7C3AED",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Comparison Operators",
      "scope": [
        "keyword.operator.comparison.formula"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Arithmetic Operators",
      "scope": [
        "keyword.operator.arithmetic.formula"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Concatenation Operator",
      "scope": [
        "keyword.operator.concatenation.formula"
      ],
      "settings": {
        "foreground": "#000000",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Relationship References",
      "scope": [
        "variable.other.relationship.formula"
      ],
      "settings": {
        "foreground": "#0070C1",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Column References",
      "scope": [
        "variable.other.column.formula"
      ],
      "settings": {
        "foreground": "#001080"
      }
    },
    {
      "name": "Parentheses",
      "scope": [
        "punctuation.parenthesis.formula"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Comma",
      "scope": [
        "punctuation.separator.comma.formula"
      ],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Dot Accessor",
      "scope": [
        "punctuation.accessor.dot.formula"
      ],
      "settings": {
        "foreground": "#000000"
      }
    }
  ]
}