{
  "name": "formula-language-support",
  "displayName": "Formula Language Support",
  "description": "Syntax highlighting for Formula language files",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "formula",
    "syntax",
    "highlighting",
    "sql",
    "database"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:formula"
  ],
  "contributes": {
    "languages": [
      {
        "id": "formula",
        "aliases": [
          "Formula",
          "formula"
        ],
        "extensions": [
          ".formula"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "formula",
        "scopeName": "source.formula",
        "path": "./syntaxes/formula.tmGrammar.json"
      }
    ],
    "themes": [
      {
        "label": "Formula Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/formula-dark-color-theme.json"
      },
      {
        "label": "Formula Light",
        "uiTheme": "vs",
        "path": "./themes/formula-light-color-theme.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "typescript": "^4.4.0"
  },
  "repository": {
    "type": "git",
    "url": "local"
  },
  "author": "Formula Language Team",
  "license": "MIT",
  "private": true
}