{
  "name": "js-to-sql-formula-compiler",
  "version": "1.0.0",
  "description": "JavaScript-based Excel-like formula compiler that converts formulas to PostgreSQL SQL",
  "type": "module",
  "main": "formula-compiler.js",
  "scripts": {
    "test": "node tests/run-all-tests.js",
    "exec-all": "./exec-formula --output markdown examples/table/submission/*.formula > exec-results.md",
    "vscode-extension": "node scripts/generate-vscode-grammar.js",
    "install-vscode-extension": "npm run vscode-extension && scripts/install-vscode-extension.sh"
  },
  "dependencies": {
    "pg": "^8.11.0",
    "dotenv": "^16.4.7"
  },
  "keywords": ["formula", "compiler", "sql", "postgresql", "excel", "lexer", "parser"],
  "author": "",
  "license": "MIT"
} 